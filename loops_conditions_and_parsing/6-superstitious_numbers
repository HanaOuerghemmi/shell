#/usr/bin/env bash
#display 4 bad luck from china 9 bad luck from japan 17 bad luck from itlay with while and case
i=0
while [ $i -le 19 ]
do
	((i++))
	case $i in 
	4)
		echo "$i"
		echo  "bad luck from China"
		;;
    	9)
    		echo "$i"
    		echo  "bad luck from Japan"
    		;;
    	17)
    		echo "$i"
    		echo "bad luck from Italy"
    		;;
    	*)
        	echo $i
        	;;
	esac
done

